@prefix ns1: <http://example.org/mynamespace/> .
@prefix ns2: <http://example.org/spatial/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/room/windowNearDoor> a ns1:Object ;
    ns1:description "no"^^xsd:string .

<http://example.org/room/windowOnFrontWall> a ns1:Object ;
    ns1:description "yes"^^xsd:string .

<http://example.org/room/behindYou> a ns1:Object ;
    ns1:description "door"^^xsd:string ;
    ns2:safePath <http://example.org/room/centerOfRoom> .

<http://example.org/room/centerOfRoom> a ns1:Object ;
    ns1:description "pantry"^^xsd:string ;
    ns2:near <http://example.org/room/farLeft>,
        <http://example.org/room/farRight>,
        <http://example.org/room/inFront>,
        <http://example.org/room/toTheLeft>,
        <http://example.org/room/toTheRight> .

<http://example.org/room/farLeft> a ns1:Object ;
    ns1:description "chair"^^xsd:string .

<http://example.org/room/farRight> a ns1:Object ;
    ns1:description "cupboard"^^xsd:string .

<http://example.org/room/inFront> a ns1:Object ;
    ns1:description "sofa"^^xsd:string ;
    ns2:oppositeOf <http://example.org/room/behindYou> .

<http://example.org/room/toTheLeft> a ns1:Object ;
    ns1:description "table"^^xsd:string ;
    ns2:adjacentTo <http://example.org/room/windowOnLeftWall> .

<http://example.org/room/toTheRight> a ns1:Object ;
    ns1:description "coffee table"^^xsd:string ;
    ns2:adjacentTo <http://example.org/room/windowOnRightWall> .

<http://example.org/room/windowOnLeftWall> a ns1:Object ;
    ns1:description "no"^^xsd:string .

<http://example.org/room/windowOnRightWall> a ns1:Object ;
    ns1:description "no"^^xsd:string .

