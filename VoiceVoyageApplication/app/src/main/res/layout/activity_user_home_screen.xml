<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A5DAF3">


    <Button
        android:id="@+id/button_instructions"
        android:layout_width="320dp"
        android:layout_height="124dp"
        android:layout_marginTop="156dp"
        android:backgroundTint="#CF9FFF"
        android:text="INSTRUCTIONS"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_settings"
        android:layout_width="320dp"
        android:layout_height="124dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="128dp"
        android:text="Settings"
        android:backgroundTint="#CF9FFF"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_navigation"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_navigation"
        android:layout_width="320dp"
        android:layout_height="124dp"
        android:layout_marginTop="32dp"
        android:text="Object Detection And Navigation"
        android:backgroundTint="#CF9FFF"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_instructions" />

</androidx.constraintlayout.widget.ConstraintLayout>
